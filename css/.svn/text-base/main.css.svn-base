	background: -o-linear-gradient(top, #000000 0%,#000000 49%,#1b1b1b 50%,#1b1b1b 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, #000000 0%,#000000 49%,#1b1b1b 50%,#1b1b1b 100%); /* IE10+ */
	background: linear-gradient(top, #000000 0%,#000000 49%,#1b1b1b 50%,#1b1b1b 100%); /* W3C */

	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#000000', endColorstr='#1b1b1b',GradientType=0 ); /* IE6-9 */
