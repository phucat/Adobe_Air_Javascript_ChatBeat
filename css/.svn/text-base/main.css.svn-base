@font-face { 
font-family: 'CB Font'; src: url(../assets/fonts/MyriadPro-Regular.otf)
}
/* Let's get this party started */
::-webkit-scrollbar {
    width: 5px;
}
 
/* Track */
::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); 
    -webkit-border-radius: 10px;
    border-radius: 10px;
}
 
/* Handle */
::-webkit-scrollbar-thumb {
    -webkit-border-radius: 10px;
    border-radius: 10px;
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(59,103,158,1)), color-stop(50%,rgba(43,136,217,1)), color-stop(51%,rgba(32,124,202,1)), color-stop(100%,rgba(125,185,232,1))); /* Chrome,Safari4+ */
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.5); 
}
::-webkit-scrollbar-thumb:window-inactive {
	background: rgba(255,0,0,0.4); 
}
input[type=checkbox], input[type=radio] {
	display: inline-block;
	font-size: 15px;
	line-height: 1em;
	margin: 0 0.25em 0 0;
	padding: 0;
	width: 1.25em;
	height: 1.25em;
	-webkit-border-radius: 0.25em;
	vertical-align: text-top;
}

input[type=radio] {
	-webkit-border-radius: 2em; /* Make radios round */
}

input[type=checkbox]:checked {
   background: url("data:image/png,%89PNG[...]") no-repeat center center;
   -webkit-background-size: 28px 28px; /* shrink high resolution background */
}

input[type=text] {
	-webkit-border-radius: 20px;
	padding:5px;
	outline: none;
	margin:10px 0;
	
}
tbody td,  tbody tr{
	cursor:pointer;
}

div, span, li {
	cursor:default;
}
tr, td {
	vertical-align:top;
}
html {
	-webkit-font-smoothing:  antialiased;
	overflow:hidden;
}
body {
	font-family: Tahoma, Geneva, sans-serif;
	font-size:10px;
	font-weight:bold;
	color:#fff;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;
	-webkit-font-smoothing:  antialiased;
	overflow:hidden;
}
#wrapper  {
	background: #000;
	-webkit-border-radius: 20px;
	width:100%;
	height:650px;
	-webkit-box-shadow: 0px 0px 28px #000000;
	position:relative;
	margin:20px;
	overflow:hidden;
	border:#333333 solid 1px;
	opacity:.97;
}


#logo {
	position:absolute;
	top : 10px;
	left: 10px;
	z-index:9;
}

img {
	-webkit-user-drag: none;
}
#moveHandle {
	height:70px;
	border-bottom: #2b3941 solid 1px;
	width:100%;
	position:absolute;
	top:0;
	left:0;

	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#1b1b1b), color-stop(49%,#1b1b1b), color-stop(50%,#000000), color-stop(100%,#000000)); /* Chrome,Safari4+ */

	-webkit-border-top-left-radius: 20px;
	-webkit-border-top-right-radius: 20px;	
}

#contents {
	position:absolute;
	top:70px;
	left:0;
	width:100%;
	height:500px;
}
#windowControls {
	position:absolute;
	top : 15px;
	right: 10px;
	font-size:9px;
	z-index:11;
}

#windowControls li {
	float:left;
	width:7px;
	height:7px;
	list-style:none;
	cursor: default;
	margin-right:10px;
