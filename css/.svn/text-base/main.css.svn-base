}

#windowControls .minimize {
	background:url(../assets/images/min_normal.png) no-repeat;
}

#windowControls .maximize {
	background:url(../assets/images/max_normal.png) no-repeat;
}


#windowControls .close {
	background:url(../assets/images/close_normal.png) no-repeat;
}

#windowControls .minimize:hover {
	background:url(../assets/images/min_hover.png) no-repeat;
}

#windowControls .maximize:hover {
	background:url(../assets/images/max_hover.png) no-repeat;
}


#windowControls .close:hover {
	background:url(../assets/images/close_hover.png) no-repeat;
}
#windowControls .minimize:active {
	background:url(../assets/images/min_active.png) no-repeat;
}

#windowControls .maximize:active {
	background:url(../assets/images/max_active.png) no-repeat;
}


#windowControls .close:active {
	background:url(../assets/images/close_active.png) no-repeat;
